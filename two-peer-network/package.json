    "type": "terminal"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:node": "node index.js",
    "start:pear": "pear run -d ."
  },
  "imports": {
    "fs": {
      "bare": "bare-fs",
      "default": "fs"
    },
    "fs/*": {
